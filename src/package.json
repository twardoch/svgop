{
    "name": "svgop",
    "version": "0.8.0",
    "description": "Binary executable versions of svgo, the Nodejs-based tool for optimizing SVG vector graphics files",
    "keywords": [
        "svgo",
        "svg",
        "optimize",
        "minify"
    ],
    "homepage": "https://github.com/twardoch/svgop",
    "bugs": {
        "url": "https://github.com/twardoch/svgop/issues",
        "email": "adam+github@twardoch.com"
    },
    "author": {
        "name": "Adam Twardoch",
        "email": "adam+github@twardoch.com",
        "url": "https://github.com/github"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/twardoch/svgop.git"
    },
    "main": "./lib/svgo.js",
    "bin": {
        "svgop": "./src/index.js"
    },
    "directories": {
        "src": "./src",
        "lib": "./lib"
    },
    "scripts": {
        "clean": "rm dist/svgop.js",
        "build-dev": "webpack -d --mode development",
        "build-prod": "webpack -p --mode production"
    },
    "dependencies": {
        "get-stdin": "^7.0.0"
    },
    "engines": {
        "node": ">=4.0.0"
    },
    "pkg": {
        "scripts": [
            "./svgo/lib/**/*.js"
        ]
    },
    "license": "Apache-2.0",
    "devDependencies": {
        "svgo": "^1.3.2",
        "webpack": "^5.0.0-beta.13",
        "webpack-bundle-analyzer": "^3.3.2"
    }
}
